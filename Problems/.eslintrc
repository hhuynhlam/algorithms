{
  "extends": "airbnb-base",

  "plugins": [
    "jasmine"
  ],

  "env": {
    "jasmine": true
  },

  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always",
      "objects": "always",
      "imports": "always",
      "exports": "always",
      "functions": "ignore"
    }],
    "no-console": ["off"],
    "no-param-reassign": ["off"]
  }
}
